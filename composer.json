{
  "name": "corex/docs",
  "description": "CoreX Documentation Website using CodeIgniter 4",
  "type": "project",
  "require": {
      "php": ">=7.4",
      "codeigniter4/framework": "^4.0"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/"
      }
  },
  "scripts": {
      "post-install-cmd": [
          "@composer dump-autoload"
      ],
      "post-update-cmd": [
          "@composer dump-autoload"
      ]
  },
  "config": {
      "preferred-install": "dist",
      "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
